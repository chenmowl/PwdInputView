<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PwdInput">
        <attr name="pwdLength" format="integer" />
        <attr name="borderColor" format="color|reference" />
        <attr name="cursorColor" format="color|reference" />
        <attr name="isCursorEnable" format="boolean" />
        <attr name="isCipherEnable" format="boolean" />
        <attr name="pwdPadding" format="dimension|reference" />
        <attr name="borderWidth" format="dimension|reference" />
        <attr name="mode">
            <enum name="cheek" value="1001" />
            <enum name="underline" value="1002" />
        </attr>
    </declare-styleable>

</resources>